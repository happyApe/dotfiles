{
  "title": "Control + Option + Letter -> Launch Application",
  "rules": [
    {
      "description": "Control + Option + Letter -> Launch Application",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "control",
                "option"
              ],
              "optional": ["caps_lock"]
            }
          },
          "to": [{"shell_command": "open '/System/Applications/Utilities/Activity Monitor.app'"}]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": [
                "control",
                "option"
              ],
              "optional": ["caps_lock"]
            }
